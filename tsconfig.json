{
  "compilerOptions": {
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "types": [
      "vite/client"
    ],
    "noEmit": true,
    "isolatedModules": false,
    "resolveJsonModule": true,
  },
  "include": [
    "vite.config.ts"
  ]
}