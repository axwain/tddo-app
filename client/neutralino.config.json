{
  "applicationId": "ax.tddo.app",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resource/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "events.on",
    "filesystem.copyFile",
    "filesystem.createDirectory",
    "filesystem.readDirectory",
    "filesystem.readFile",
    "filesystem.writeFile",
    "init",
    "os.getEnv",
    "window.move",
    "window.setDraggableRegion"
  ],
  "modes": {
    "window": {
      "title": "Tddo App",
      "width": 580,
      "height": 400,
      "resizable": false,
      "borderless": true,
      "icon": "/resource/tddo-app128.png"
    }
  },
  "cli": {
    "binaryName": "tddo-app",
    "resourcesPath": "/resource/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}