{
  "applicationId": "ax.tddo.app",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resource/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.copyFile",
    "filesystem.createDirectory",
    "filesystem.readDirectory",
    "filesystem.readFile",
    "filesystem.writeFile",
    "os.getEnv"
  ],
  "modes": {
    "window": {
      "title": "client",
      "width": 580,
      "height": 400,
      "resizable": false
    }
  },
  "cli": {
    "binaryName": "client",
    "resourcesPath": "/resource/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}